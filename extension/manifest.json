{
    "name": "ThinToken Browser Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://*.google.com/*"],
          "js": [
            "content.js"
          ]
        }
      ],
    "options_ui":{
    
      "page":"extButton.html"
    },
    "action": {
      "default_popup": "extButton.html"
    }
  }