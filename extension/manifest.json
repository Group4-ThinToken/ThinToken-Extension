{
  "name": "ThinToken Browser Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://accounts.google.com/signin/v2/challenge/totp?*"

      ],
      "js": [
        "constants.js",
        "./util/encryption.js",
        "./util/bluetooth.js",
        "./ContentScripts/otpFlow.js"
      ]
    }, {
      "matches": [
        "https://accounts.google.com/v3/signin/*",
        "https://login.live.com/login.srf?*"
      ],
      "js": [
        "constants.js",
        "./util/encryption.js",
        "./util/bluetooth.js",
        "./ContentScripts/preOtpFlow.js"
      ]
    }, {
      "matches": [
        "https://myaccount.google.com/two-step-verification/authenticator?*"
      ],
      "js": [
        "constants.js",
        "./util/encryption.js",
        "./util/bluetooth.js",
        "./ContentScripts/confirmFlow.js"
      ]
    }
  ],
  "action": {
    "default_popup": "./Popup/popup.html"
  },
  "permissions": [
    "storage"
  ]
}