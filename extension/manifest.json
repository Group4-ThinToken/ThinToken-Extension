{
    "name": "ThinToken Browser Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://accounts.google.com/signin/v2/challenge/totp?*"],
          "js": [
            "constants.js",
            "./util/encryption.js",
            "./util/bluetooth.js",
            "content.js"
          ]
        }
      ],
    "action": {
      "default_popup": "./Popup/popup.html"
    }
  }